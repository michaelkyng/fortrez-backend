{
  "version": 2,
  "builds": [
    {
      "src": "src/main.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api(/?)(.*)",
      "dest": "/src/main.ts"
    },
    {
      "src": "/api-json",
      "dest": "/src/main.ts"
    },
    {
      "src": "/favicon.ico",
      "status": 200,
      "headers": { "Cache-Control": "public, max-age=3600" }
    },
    {
      "src": "/(.*)",
      "dest": "/src/main.ts",
      "methods": ["GET", "POST", "PUT", "DELETE", "PATCH", "OPTIONS"]
    }
  ]
}